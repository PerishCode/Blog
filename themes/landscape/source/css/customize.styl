#header
    height: 200px

#footer
    padding: 30px 0

#logo
    font-weight: bolder


#wrap
    overflow: overlay

    &::-webkit-scrollbar
        width: 6px
        height: 6px

    &::-webkit-scrollbar-thumb
        background-color: rgba(255,255,255,0.3)
        border-radius: 6px

        &:hover
            background-color: rgba(255,255,255,0.7)

    &::-webkit-scrollbar-track
        background-color: rgba(0,0,0,0)